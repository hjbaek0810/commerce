{
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "all",
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": [
        ".ts",
        ".tsx"
      ],
      "options": {
        "parser": "babel-ts"
      }
    },
    {
      "files": [
        ".js",
        ".jsx"
      ],
      "options": {
        "parser": "babel"
      }
    }
  ]
}
